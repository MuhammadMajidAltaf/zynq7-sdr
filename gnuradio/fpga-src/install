cp include/test1/toto.h /tftpboot/apf51-root/usr/include/test1/
cp include/test1/dds_fpga.h /tftpboot/apf51-root/usr/include/test1/

cp build_cross/lib/libgnuradio-test1.so /tftpboot/apf51-root/usr/lib/
cp build_cross/swig/_test1_swig.so /tftpboot/apf51-root/usr/lib/python2.7/site-packages/test1/
cp build_cross/swig/test1_swig.py /tftpboot/apf51-root/usr/lib/python2.7/site-packages/test1/
cp build_cross/swig/test1_swig.pyc /tftpboot/apf51-root/usr/lib/python2.7/site-packages/test1/
cp build_cross/swig/test1_swig.pyo /tftpboot/apf51-root/usr/lib/python2.7/site-packages/test1/

cp swig/test1_swig.i /tftpboot/apf51-root/usr/include/test1/test1/swig/
